window/size/viewport_height=600
window/size/resizable=false
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="godot-gdrl"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_rl_agents/plugin.cfg")

